<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.Double?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.LineTo?>
<?import javafx.scene.shape.MoveTo?>
<?import javafx.scene.shape.Path?>
<?import javafx.scene.shape.Polygon?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: green;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane prefHeight="600.0" prefWidth="1280.0">
         <children>
            <Polygon fx:id="path" fill="BROWN" stroke="BLACK" strokeType="INSIDE">
              <points>
                <Double fx:value="0.0" /> <Double fx:value="100.0" /> 
                <Double fx:value="400.0" /> <Double fx:value="100.0" />
                <Double fx:value="400.0" /> <Double fx:value="400.0" />
                <Double fx:value="900.0" /> <Double fx:value="400.0" />
                <Double fx:value="900.0" /> <Double fx:value="200.0" />
                <Double fx:value="1280.0" /> <Double fx:value="200.0" />
                <Double fx:value="1280.0" /> <Double fx:value="250.0" />
                <Double fx:value="950.0" /> <Double fx:value="250.0" />
                <Double fx:value="950.0" /> <Double fx:value="450.0" />
                <Double fx:value="350.0" /> <Double fx:value="450.0" />
                <Double fx:value="350.0" /> <Double fx:value="150.0" />
                <Double fx:value="0.0" /> <Double fx:value="150.0" />
              </points>
            </Polygon>
            <Path fx:id="line" stroke="BLACK">
                <elements>
                    <MoveTo x="0.0" y="125.0" />
                    <LineTo x="375.0" y="125.0" />
                    <LineTo x="375.0" y="425.0" />
                    <LineTo x="925.0" y="425.0" />
                    <LineTo x="925.0" y="225.0" />
                    <LineTo x="1280" y="225.0" />
                </elements>
            </Path>
         </children>
      </Pane>
      <ButtonBar prefHeight="120.0" prefWidth="1280.0" style="-fx-background-color: gray; -fx-border-color: blue;" translateY="600.0">
         <buttons>
            <Button mnemonicParsing="false" onAction="#butTest" prefHeight="75.0" prefWidth="75.0" text="GO!" />
            <Button mnemonicParsing="false" onMouseClicked="#clicked" text="Tour" />
            <Button mnemonicParsing="false" onAction="#towerContent" text="TourContent" />
         </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
